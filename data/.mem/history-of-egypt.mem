    "vector": [-0.044305, 0.097471, -0.079749, -0.062027, 0.026583, 0.115193, -0.079749, -0.008861, -0.097471, -0.079749, 0.026583, 0.079749, 0.044305, 0.079749, -0.079749, -0.097471, -0.008861, -0.221525, -0.150637, 0.168359, -0.026583, -0.292414, -0.062027, 0.132915, -0.115193, -0.026583, -0.203803, 0.044305, -0.044305, -0.256969, -0.168359, -0.221525, -0.044305, 0.097471, -0.079749, -0.062027, 0.026583, 0.115193, -0.079749, -0.008861, -0.097471, -0.079749, 0.026583, 0.079749, 0.044305, 0.079749, -0.079749, -0.097471, -0.008861, -0.221525, -0.150637, 0.168359, -0.026583, -0.292414, -0.062027, 0.132915, -0.115193, -0.026583, -0.203803, 0.044305, -0.044305, -0.256969, -0.168359, -0.221525],
    "label": "U"
