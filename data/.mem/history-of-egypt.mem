    "vector": [-0.138454, 0.080158, -0.109306, -0.123880, -0.065583, 0.138454, -0.065583, -0.167602, -0.065583, 0.021861, -0.007287, 0.080158, 0.021861, 0.123880, -0.051009, -0.153028, 0.021861, -0.138454, -0.182176, 0.123880, -0.036435, -0.240473, -0.036435, 0.021861, -0.080158, -0.109306, -0.167602, -0.036435, -0.051009, -0.167602, -0.240473, -0.269621, -0.138454, 0.080158, -0.109306, -0.123880, -0.065583, 0.138454, -0.065583, -0.167602, -0.065583, 0.021861, -0.007287, 0.080158, 0.021861, 0.123880, -0.051009, -0.153028, 0.021861, -0.138454, -0.182176, 0.123880, -0.036435, -0.240473, -0.036435, 0.021861, -0.080158, -0.109306, -0.167602, -0.036435, -0.051009, -0.167602, -0.240473, -0.269621],
