    "vector": [-0.117389, -0.052173, -0.065216, 0.026086, -0.013043, -0.143476, -0.013043, 0.039130, -0.052173, 0.026086, -0.078259, 0.026086, 0.234778, 0.104346, 0.013043, -0.117389, -0.065216, -0.156519, -0.221735, 0.104346, -0.026086, -0.195648, -0.117389, 0.065216, -0.052173, -0.052173, -0.130432, 0.052173, -0.143476, -0.156519, -0.208692, -0.326081, -0.117389, -0.052173, -0.065216, 0.026086, -0.013043, -0.143476, -0.013043, 0.039130, -0.052173, 0.026086, -0.078259, 0.026086, 0.234778, 0.104346, 0.013043, -0.117389, -0.065216, -0.156519, -0.221735, 0.104346, -0.026086, -0.195648, -0.117389, 0.065216, -0.052173, -0.052173, -0.130432, 0.052173, -0.143476, -0.156519, -0.208692, -0.326081],
    "label": "U"
